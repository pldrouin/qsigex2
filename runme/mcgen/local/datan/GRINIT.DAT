bg_col = 0,0,0
;
; PostScript
[ps]
; device_name : string identifying entry 
device_name = "PS default"
; phys_size = Width,Height (measured in cm)
phys_size = 27.1,19.125
; borders : x_offset,y_offset (measured in cm)
; distance of plot frame (lower left) from physical border
borders = 0.635,0.635
; line_style_# : Red,Green,Blue,Line Width
; RGB color values in fractions of one
; Line Width 
line_style_1 = ,,,2
line_style_2 = ,,,1
line_style_3 = ,,,4
line_style_4 = ,,,3
line_style_5 = ,,,3
line_style_6 = ,,,2
line_style_7 = ,,,3
line_style_8 = ,,,1
; bg_col: background color R,G,B; empty: transparent
bg_col = 
; output_form : ps | eps (default: ps)
output_form = eps
; eps_spec : phys - bounding box is phys_size
;            act  - bounding box is actual plot size
;            actm - bounding box is actual plot size, coordinates beg. from 0
;            actp - bounding box is actual plot size, plot in portrait mode
eps_spec = act
;
; HP-GL plotter language
[hpgl]
device_name = "HPGL default"
; phys_size = Width,Height (measured in cm)
phys_size = 27.1,19.125
; pixel_num = X_pixels,Y_pixels
pixel_num = 10900,7650
; line_style_# : Pen Number (default: #)
line_style_1 = 1
line_style_2 = 2
line_style_3 = 3
line_style_4 = 4
line_style_5 = 5
line_style_6 = 6
line_style_7 = 7
line_style_8 = 8
; output_form : pl | lj (default: pl)
output_form = pl
;
