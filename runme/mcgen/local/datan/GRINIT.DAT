; line_style_# : Red,Green,Blue
line_style_1 = 255,255,255
line_style_2 = 0,0,255
line_style_3 = 0,255,0
line_style_4 = 255,0,0
line_style_5 = 0,255,255
line_style_6 = 255,0,255
